"%1$s or %2$s" = "%1$s 或 %2$s";
"Face value" = "牌面點數";
"Hand total is 21 on the first two cards, including but not limit to:" = "兩張牌加總為 21 點，包含但不限於：";
"%d Points" = "%d點";
