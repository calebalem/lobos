/* TutorialLayer */
"_stud_tutorial_desc_intro" = "7 Card Stud klasik bir poker oyunudur. Bu oyunda her oyuncuya el boyunca 7 kart dağıtılır ve dağıtılan 7 kart arasından elindeki en iyi 5 kartla en iyi poker elini yapan oyuncu o eli kazanır.";

"_stud_tutorial_desc_intro_new" = "7 Card Stud klasik bir poker oyunudur. Bu oyunda her oyuncuya el boyunca 7 kart dağıtılır. Dağıtılan 7 kart arasından elindeki en iyi 5 kartla en iyi poker elini yapan oyuncu o eli kazanır.";
"_stud_tutorial_desc_3rd_street" = "Oyun, herkesin yüzü aşağı bakan kapalı (holde) iki kart ve yukarı bakan açık bir kart (kapı kartı) almasıyla başlar. Herkesin giriş yapmasından sonra tur en küçük kapı kartına sahip olan kişiyle başlar ve sıra bu oyuncudan itibaren masanın etrafında saat yönünde devam eder. Beraberlik durumunda sıralama renklere göre yapılır: sinek en düşüktür, sonra karo, kupa ve son olarak maça gelir. Oyuna başlayan kişi \"açılış\" adı verilen bir kör bahis yapmak zorundadır ve bu tutar önceden belirlenmiş bir açılış tutarından küçük bahis tutarına kadar değişen bir miktarda olabilir. Ön floptan ziyade, buna “3. cadde” adı verilir.";
"_stud_tutorial_desc_4th_street" = "Pot tamam olduğunda oyuncular \"4. cadde\" olarak ikinci kapı kartlarını alır. Yeni bir bahis turu başlar.";
"_stud_tutorial_desc_5th_street" = "Ardından 5. caddede yeniden birer kapı kartı dağıtılır ve bunu bir tur bahis izler.";
"_stud_tutorial_desc_6th_street" = "Bunun ardından 6. yine birer kapı kartı dağıtılır ve bunu bir tur bahis izler.";
"_stud_tutorial_desc_7th_street" = "Son olarak kapalı birer nehir kartı dağıtılır ve bunu da bir tur bahis izler. ";
"_stud_tutorial_desc_result" = "5 karttan oluşan en iyi poker eline kim sahipse, potu o kazanır.";

/* Main Tab Scene */
"End Game" = "Oyunu Bitir";

"_stud_tutorial_desc_end" = "Pozisyonun el boyunca değiştiğini unutmayın. 3. cadde dışında, en güçlü ele sahip olan kişi ilk hamleyi yapar. NLH ile karşılaştırıldığında bir diğer önemli fark, 7 Card Stud'ın bahis limitlerine sahip olmasıdır. Her oyunda bir “küçük bahis” ve bir “büyük bahis” vardır. 3. ve 4. caddelerde küçük bahis, 5., 6., 7. caddelerde ise büyük bahis oynanır. Tüm bahisler veya artırımlar küçük bahis veya büyük bahis artışlarıyla yapılır. Örneğin, oyun 10/20 ile başlarsa, 10 fişle bahis oynayabilir ve 3. ve 4. caddelerde 10 fiş artırabiliriz. Beşinci caddeye ulaştığımızda bu sayılar 20'ye çıkar. Texas Hold'em'e benzer şekilde, her oyuncu her bahis turunda bob, pey sürme, görme/artırma veya pas seçeneklerine sahiptir.";
"_stud_tutorial_desc_end_new" = "Pozisyonun el boyunca değiştiğini unutmayın. 3. cadde dışında, en güçlü ele sahip olan kişi ilk hamleyi yapar. NLH ile karşılaştırıldığında bir diğer önemli fark, 7 Card Stud'ın bahis limitlerine sahip olmasıdır. Her oyunda bir “küçük bahis” ve bir “büyük bahis” vardır. 3. ve 4. caddelerde küçük bahis, 5., 6., 7. caddelerde ise büyük bahis oynanır. Tüm bahisler veya artırımlar küçük bahis veya büyük bahis artışlarıyla yapılır. Örneğin, oyun 10/20 ile başlarsa, 10 fişle bahis oynayabilir ve 3. ve 4. caddelerde 10 fiş artırabiliriz. Beşinci caddeye ulaştığımızda bu sayılar 20'ye çıkar. Texas Hold'em'e benzer şekilde, her oyuncu her bahis turunda bob, pey sürme, görme/artırma veya pas seçeneklerine sahiptir.";

